{
    "description": "git commit时，自动使用phpcs-fixer格式化代码",
    "name": "xianlife/composer-phpcs-git-plugin",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "xianlife\\composer\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "class": "xianlife\\composer\\PhpcsPlugin"
    }
}